{
  "name": "codus-crossfit-server",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn build --watch\" \"nodemon dist/index.js\"",
    "build": "tsc",
    "db:create": "yarn build && node dist/db/tasks/create.js",
    "db:drop": "yarn build && node dist/db/tasks/drop.js",
    "db:migrate": "yarn build && node dist/db/tasks/migrate.js",
    "db:rollback": "yarn build && node dist/db/tasks/rollback.js",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "apollo-server": "^2.4.2",
    "bcrypt": "^3.0.5",
    "graphql-iso-date": "^3.6.1",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.13"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.19",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.0",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.9.4",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/pg": "^7.4.13",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "concurrently": "^4.1.0",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "nodemon": "^1.18.10",
    "typescript": "^3.3.3"
  }
}
